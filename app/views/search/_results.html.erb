<input type="hidden" name="num_pages" class="num_pages" value="<%= @search_result.num_pages%>">

<input type="hidden" name="the_tags" class="the_tags" value="<%= extract_tags(@search_result)%>">

<input type="hidden" name="n_results" class="n_results" value="<%= @search_result.total_entries%>">

<input type="hidden" class="params_type" value="<%= params[:type]%>">
<input type="hidden" class="params_language" value="<%= params[:language]%>">
<input type="hidden" class="params_tags" value="<%= params[:tags]%>">
<input type="hidden" class="params_sort_by" value="<%= params[:sort_by]%>">

<% @search_result.each do |r| %>
  <% unless r.nil? %>
    <%= render partial: "#{ r.class.to_s.tableize }/search_result",
               locals:  { r.class.to_s.underscore.to_sym => r, :prefix_id => "search" }  %>
  <%end%>
<% end %>
