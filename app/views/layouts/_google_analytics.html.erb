    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); 
      ga.type = 'text/javascript'; 
      ga.async = true;
      ga.src = '//stats.g.doubleclick.net/dc.js';
      var s = document.getElementsByTagName('head')[0]; 
      s.appendChild(ga);
    })();
  </script>
<% end %>
